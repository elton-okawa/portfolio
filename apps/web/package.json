{
  "name": "blog-app",
  "version": "0.1.0",
  "private": true,
  "prettier": "@elton-okawa/prettier-config",
  "scripts": {
    "dev": "concurrently --names 'NEXTJS,RELAY' -c 'gray,yellow.dim' 'next dev' 'pnpm relay --watch'",
    "build": "pnpm relay && next build",
    "start": "next start",
    "lint": "next lint",
    "format": "pnpm prettier --write .",
    "relay": "mkdir -p src/lib/shared/__generated__ && relay-compiler",
    "analyze": "cross-env ANALYZE=true next build"
  },
  "dependencies": {
    "@ark-ui/react": "^0.7.0",
    "@elton-okawa/graphql-schema": "workspace:^0.0.0",
    "@heroicons/react": "^2.0.18",
    "graphql": "^16.6.0",
    "graphql-yoga": "^3.9.1",
    "next": "13.4.4",
    "next-themes": "^0.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-relay": "^15.0.0",
    "relay-nextjs": "^2.0.1",
    "relay-runtime": "^15.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.1",
    "@babel/plugin-proposal-decorators": "^7.22.3",
    "@elton-okawa/eslint-config-react": "workspace:^1.0.0",
    "@elton-okawa/prettier-config": "workspace:^1.0.0",
    "@next/bundle-analyzer": "^13.4.4",
    "@types/node": "20.2.5",
    "@types/react": "18.2.8",
    "@types/react-dom": "18.2.4",
    "@types/react-relay": "^14.1.4",
    "@types/relay-runtime": "^14.1.10",
    "autoprefixer": "^10.4.14",
    "babel-plugin-relay": "^15.0.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "concurrently": "^8.0.1",
    "cross-env": "^7.0.3",
    "eslint": "8.41.0",
    "eslint-config-next": "13.4.4",
    "graphql": "^16.6.0",
    "postcss": "^8.4.24",
    "relay-compiler": "^15.0.0",
    "relay-compiler-language-typescript": "^15.0.1",
    "tailwindcss": "^3.3.2",
    "typescript": "5.0.4"
  }
}
